msgid "Assign to group:"
msgstr "Assegnal al gruppo:"

#: class-gf-mailup.php:505
msgid "Always"
msgstr "Sempre"

#: class-gf-mailup.php:509
msgid "If"
msgstr "Se"

#: class-gf-mailup.php:1131 class-gf-mailup.php:1149
msgid "No list related"
msgstr "Nessuna lista relazionata"

